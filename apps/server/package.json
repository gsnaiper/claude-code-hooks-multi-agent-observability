{
  "name": "multi-agent-observability-server",
  "version": "1.0.0",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "typecheck": "tsc --noEmit",
    "migrate": "bun run scripts/migrate-to-postgres.ts",
    "migrate:dry-run": "bun run scripts/migrate-to-postgres.ts --dry-run",
    "migrate:no-embeddings": "bun run scripts/migrate-to-postgres.ts --skip-embeddings",
    "merge": "bun run scripts/full-merge-migration.ts",
    "merge:dry-run": "bun run scripts/full-merge-migration.ts --dry-run",
    "merge:no-embeddings": "bun run scripts/full-merge-migration.ts --skip-embeddings"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/ws": "^8.5.13",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@lancedb/lancedb": "^0.23.0",
    "@xenova/transformers": "^2.17.2"
  }
}
